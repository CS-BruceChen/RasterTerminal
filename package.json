{
  "name": "epjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pkg": "pkg . --out-path=pkg_package/"
  },
  "dependencies": {
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "prom-client": "^14.2.0",
    "rtmodule-windows": "^0.0.1"
  },
  "bin": "./app.js",
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "dist/**/*"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64"
    ]
  }
}
